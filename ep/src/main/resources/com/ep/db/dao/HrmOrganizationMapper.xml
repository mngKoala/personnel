  <select id="getOrganizationWithPositionTree" resultMap="getSubNodeWithPostion">
    SELECT * FROM (
	 select * from hrm_organization where  deleted = 0 and parent_id = #{id}   order by sort) a
    UNION all
   SELECT * FROM (
    select id,'' as code  ,short_name,full_name ,'position' as type  ,'' as parent_id ,valid ,'' as hierarchy ,
          sort  ,company_id,tenant_id ,create_by ,create_time ,
          update_by ,update_time ,deleted, delete_by ,delete_time,
          '' as version   ,'' as notes
    from hrm_position
    where  deleted = 0 and org_id = #{id} order by sort) b
  </select>

  <!--根据父id获取子节点-->
  <select id="getParentTree" resultMap="getParentNode">
	 select * from hrm_organization where  deleted = 0 and id = #{id}   order by sort
  </select>
